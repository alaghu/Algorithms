class GreatestCommonDivisor

  def find_the_greatest_commom_divisor (a, b)
    if is_the_value_an_integer(a)
      if is_the_value_an_integer(b)

        remainder = a % b

        if remainder == 0
          greatest_common_divisor = b
          puts "he greatest common divisor or #{a} is  #{greatest_common_divisor}"
        else
          a = b
          b = remainder
        end

      end
    end
  end

  #Obtain two inputs a and b
  # Check if they are integeres, else error out
  # Divide a by b.
  # Get Remainder
  # Remainder , r would be between 0 <= r <= b
  # if r == 0 then end
  # else set a == b, b == r , repeat


end

